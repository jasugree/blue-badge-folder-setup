:0,"fixableWarningCount":0},{"filePath":"84","messages":"85","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\index.js",[],["86","87"],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\reportWebVitals.js",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\site\\Header.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\site\\Footer.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\site\\Sidebar.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\site\\Resources.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\site\\Home.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\concepts\\FunctionalComponentDemo.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\concepts\\JSXRules.jsx",["88"],"import React from \"react\";\r\n\r\nconst JSXRules = () => {\r\n    return (\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <h1 className=\"section-title\">JSX</h1>\r\n                <dl>\r\n                    <dt>Resembles HTML</dt>\r\n                    <dd>It's not. It's actually closer to JavaScript.</dd>\r\n                    <dt>React Elements</dt>\r\n                    <dd>These are used to construct and update the DOM, or what  you see on the screen.</dd>\r\n                    <dt>Not Required</dt>\r\n                    <dd>You can write in vanilla JS, but most people use JSX.</dd>\r\n                </dl>\r\n                <hr />\r\n                <NormalComponent />\r\n                <CreateElementComponent />\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default JSXRules;\r\n\r\nconst NormalComponent = () => {\r\n    return(\r\n        <div style={{border: '2px solid black'}}>\r\n            <h1>Normal Functional Component</h1>\r\n            <p>This was constructed with JSX in the return.</p>\r\n            <img src=\"https://pbs.twimg.com/media/DOzL82mXkAA0zFs.jpg\"/ >\r\n        </div>\r\n    )\r\n}\r\n\r\nconst CreateElementComponent = () => {\r\n  return React.createElement(\r\n    \"div\",\r\n    {style: {border: \"2px solid black\"}},\r\n    React.createElement(\"h1\", null, \"CreateElement Component\"),\r\n    React.createElement(\r\n      \"p\",\r\n      null,\r\n      \"This was contructed with CreateElement calls in the return.\"\r\n    ),\r\n    React.createElement(\r\n      \"img\",\r\n      {\r\n        src: \"https://miro.medium.com/max/786/1*jJZHFQmhkq_7ohn18KrMhA.png\",\r\n      },\r\n      null\r\n    )\r\n  );\r\n};\r\n","C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\concepts\\States.jsx",["89"],"import React, {useState} from 'react';\r\n\r\nconst State =()=> {\r\n\r\n    return(\r\n\r\n        \r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <ul>\r\n                    <dt>useState is unique to functional components</dt>\r\n                    <dd>Calss compnents have a sperate means of using and changing state.</dd>\r\n                    <dt>useState Uses Array Destructuring</dt>\r\n                    <dd>useState proves a state variable and a setState funtion</dd>\r\n                    <dt>useState is a Hook</dt>\r\n                    <dd>useState is a hook baked into React</dd>\r\n                    <dt>Triggers Re-renders</dt>\r\n                    <dd>Like with props changes, changing the state of a compenent re-renders the whole compenent.</dd>\r\n                </ul>\r\n                <StateExample />\r\n            </div>\r\n        </div>\r\n\r\n        \r\n    )\r\n}\r\n\r\nexport default State\r\n\r\n\r\nfunction StateExample(){\r\n    const [text, setText] = useState('initial value ');\r\n    const [likeNum, setLikeNum] = useState(0);\r\n    const [textColor, setTextColor] = useState('blue');\r\n\r\n    return(\r\n        <div>\r\n            <input value={text} onChange={e => setText(e.target.value)}/>\r\n            <br />\r\n            <img style={{ width: '100px', height: '100px'}} src=\"https://upload.wikimedia.org/wikipedia/commons/1/13/Facebook_like_thumb.png\"  onClick={e => setLikeNum(likeNum +1)}/>\r\n            <span>{likeNum}</span>\r\n            <br />\r\n            <br />\r\n            <p\r\n            style={{color: textColor}}\r\n            onMouseEnter={e => setTextColor('red')}\r\n            onMouseLeave={e => setTextColor('blue')}\r\n            >This text should be able to change\r\n            </p>\r\n        </div>\r\n    )\r\n}","C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\concepts\\Effects.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\concepts\\PropsDemo.jsx",["90","91","92"],"import React, {useState} from 'react';\r\nimport PropTypes from 'prop-types';\r\n\r\nconst PropsDemo = () => {\r\n\r\n    const [color, setColor] = useState('white');\r\n    const [background, setBackground] = useState('purple');\r\n    const [borderRadius, setBorderRadius] = useState('5px');\r\n    const [borderStyle, setBorderStyle] = useState('dashed');\r\n    const [display, setDisplay] = useState('inline-block');\r\n    const [width, setWidth] = useState('350px');\r\n    const [textAlign, setTextAlign] = useState('center');\r\n\r\n    let styles ={\r\n        color: color,\r\n        backgroundColor: background,\r\n        borderRadius: borderRadius,\r\n        borderStyle: borderStyle,\r\n        display: display,\r\n        width: width,\r\n        textAlign: textAlign\r\n\r\n    }\r\n\r\n    const toggleColor = () => {\r\n        color === 'white' ? setColor('pink') : setColor('white');\r\n    }\r\n    const toggleBackgroundColor = () => {\r\n        background === 'purple' ? setBackground('black') : setBackground('purple');\r\n    }\r\n    const toggleBorderRadius = () => {\r\n        borderRadius === '5px' ? setBorderRadius('20px') : setBorderRadius('5px');\r\n    }\r\n    const toggleBorderStyle = () => {\r\n        borderStyle === 'dashed' ? setBorderStyle('double') : setBorderStyle('dashed');\r\n    }\r\n\r\n\r\n    return (\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <div style={styles}>\r\n                <FunctionalComponent string = \"Color Toggle\" function={toggleColor} selectedStyle={color}/>\r\n                <FunctionalComponent string = \"Background Color Toggle\" function={toggleBackgroundColor}  selectedStyle={background} />\r\n                <FunctionalComponent string = \"Border Radius Toggle\"  function={toggleBorderRadius}  selectedStyle={borderRadius}/>\r\n                <FunctionalComponent string = \"Border Style Toggle\" function={toggleBorderStyle}  selectedStyle={borderStyle} />\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default PropsDemo;\r\n\r\nconst FunctionalComponent = (props) => {\r\n    return (\r\n        <div>\r\n            <p>{props.string}</p>\r\n            <button onClick={props.function}>Press Me!</button>\r\n            <TinyComponent selectedStyle={props.selectedStyle} />\r\n        </div>\r\n    )\r\n}\r\n\r\nconst TinyComponent = (props) =>{\r\n    return (\r\n        <div>\r\n            <p>The Current style is: {props.selectedStyle}</p>\r\n        </div>\r\n    )\r\n}\r\n\r\nFunctionalComponent.defaultProps = {\r\n    string: 'This is wild!',\r\n    function: () => console.log(\"can I see this in my dev tools?\"),\r\n    selectedStyle: 'what style???'\r\n}\r\n\r\nFunctionalComponent.propTypes = {\r\n    string: PropTypes.string.isRequired,\r\n    function: PropTypes.func.isRequired,\r\n    selectedStyle: PropTypes.string.isRequired\r\n}","C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\apps\\timer-apps\\TimerApp.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\apps\\timer-apps\\TimePieceApps.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\apps\\timer-apps\\ClockApp.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\apps\\timer-apps\\StopWatchApp.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\components\\apps\\timer-apps\\ClockAppCalifornia.jsx",[],"C:\\Users\\Jacob Greene\\Desktop\\Coding\\ElevenFiftyProjects\\BlueBadge\\blue-badge-folder-setup\\react-modules\\react-fundamentals\\src\\App.jsx",[],"C:\\Users\\Jacob Green